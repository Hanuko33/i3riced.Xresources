!! URxvt Appearance
urxvt*depth: 32

URxvt.font: xft:UbuntuMono Nerd Font:size=15, \
            xft:Inconsolata Nerd Font Mono:size=15, \
            xft:DejaVuSansMono Nerd Font:size=15, \
            xft:DroidSansMono Nerd Font:size=15, \
            xft:FuraMono Nerd Font:size=15, \
            xft:RobotoMono Nerd Font:size=15, \
            xft:Noto Sans Mono:size=15, \
            xft:Monospace:style=Medium:size=15  

URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.geometry: 92x24
URxvt.internalBorder: 24
URxvt.cursorBlink: true
URxvt.cursorUnderline: false
URxvt.saveline: 2048
URxvt.scrollBar: false
URxvt.scrollBar_right: false
URxvt.urgentOnBell: true
URxvt.iso14755: false

URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C
